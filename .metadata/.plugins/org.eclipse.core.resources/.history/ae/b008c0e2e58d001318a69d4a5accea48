import java.io.*;
import java.util.*;

public class main {

	private Scanner x;
	
	public void openFile (){
		try {
			x= new Scanner(new File ("data.txt"));
			
		}
		catch(Exception e){
			System.out.println("could not find file");
		
	}

	}
	
	public void readFile(){
		
		while(x.hasNext()){
			String a = x.next();
			
			System.out.println(a);
		}
	}

	public void closeFile(){
		x.close();
		
	}
	
}
